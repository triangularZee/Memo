#include <iostream>
#include <vector>
#include <map>
#include <string>

using namespace std;

void solve(string str){
    map<string, string> m;
    
    for(int i=0; i<str.size(); i++){
        m.insert(pair<string,string>(str.substr(i,str.size()),str.substr(i,str.size())));
    }
  
    for(auto it = m.begin(); it != m.end(); it++){
        cout << it->first << '\n';
    }
}

int main(){
    string str;
    cin >> str;
    solve(str);
    return 0;
}
