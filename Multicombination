#include <iostream>
#include <vector>
#define MAX 5

using namespace std;

vector<int> arr;
vector<int> iv;


void show(){
  for(int i=0; i<iv.size(); i++){
      cout << iv[i] << ' ';
  }
  cout << '\n';
}

void DFS(int idx, int cnt){

  if(cnt == 3) {// n = 뽑는 갯수
    show();
    return;
   }
   
   for(int i = idx; i < MAX; i++){ // i가 idx을 시점을 삼는 것이 조합
    iv.push_back(arr[i]);
    DFS(i, cnt+1);
    iv.pop_back();
    }
}

int main(){
    arr.push_back(1);
    arr.push_back(2);
    arr.push_back(3);
    arr.push_back(4);
    arr.push_back(5);
    
    DFS(0,0);
    return 0;
    
}
